{"ast":null,"code":"import request from '@/utils/request';\nexport default {\n  name: 'adddrone',\n  data() {\n    return {\n      user: Cookie.get('user') ? JSON.parse(Cookie.get('user')) : {},\n      form: {\n        cover: ''\n      },\n      categories: [],\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入无人机名称',\n          trigger: 'blur'\n        }],\n        droneNo: [{\n          required: true,\n          message: '请输入无人机标准码',\n          trigger: 'blur'\n        }],\n        score: [{\n          required: true,\n          message: '请输入积分',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    request.get('/category/tree').then(res => {\n      this.categories = res.data;\n    });\n  },\n  methods: {\n    handleCoverSuccess(res) {\n      if (res.code === '200') {\n        // this.$set(this.form,'cover',res.data)\n        this.form.cover = res.data;\n      }\n    },\n    save() {\n      this.$refs['ruleForm'].validate(valid => {\n        if (valid) {\n          request.post('/drone/save', this.form).then(res => {\n            console.log(this.form);\n            if (res.code === '200') {\n              this.$notify.success('新增成功');\n              this.$refs['ruleForm'].resetFields();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","name","data","user","Cookie","get","JSON","parse","form","cover","categories","rules","required","message","trigger","droneNo","score","created","then","res","methods","handleCoverSuccess","code","save","$refs","validate","valid","post","console","log","$notify","success","resetFields","error","msg"],"sources":["src/views/drone/DroneAdd.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>增加无人机</h2>\r\n    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" :inline=\"true\" style=\"width: 70%\" label-width=\"100px\">\r\n      <el-form-item label=\"无人机名称\" prop=\"name\">\r\n        <el-input v-model=\"form.name\" placeholder=\"请输入无人机名称\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"描述\" prop=\"description\">\r\n        <el-input style=\"width:400px\" v-model=\"form.description\" type=\"textarea\" placeholder=\"请输入描述\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"创造公司\" prop=\"company\">\r\n        <el-input v-model=\"form.company\" placeholder=\"请输入创造公司\"></el-input>\r\n      </el-form-item>\r\n\r\n\r\n      <el-form-item label=\"创造时间\" prop=\"createtime\">\r\n        <el-date-picker\r\n            v-model=\"form.createtime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请输入创造时间\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"分类\" prop=\"category\">\r\n        <el-cascader\r\n            style=\"width:200px\"\r\n            :props=\"{value:'name' ,label:'name'}\"\r\n            v-model=\"form.categories\"\r\n            :options=\"categories\"\r\n        ></el-cascader>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"无人机编号\" prop=\"droneNo\">\r\n        <el-input v-model=\"form.droneNo\" placeholder=\"请输入无人机编号\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"所能极限飞行距离\" prop=\"score\">\r\n        <el-input v-model=\"form.score\" placeholder=\"请输入所能极限飞行距离\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"图片\" prop=\"cover\">\r\n        <el-upload\r\n            class=\"avatar-uploader\"\r\n            :action=\"'http://localhost:9090/api/drone/file/upload?token='+this.user.token\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleCoverSuccess\"\r\n\r\n        >\r\n          <img v-if=\"form.cover\" :src=\"form.cover\" class=\"avatar\">\r\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </el-form-item>\r\n\r\n\r\n    </el-form>\r\n\r\n    <div style=\"text-align:center; margin-top:30px;\">\r\n      <el-button type=\"primary\" @click=\"save\">提交</el-button>\r\n      <el-button type=\"danger\">取消</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from '@/utils/request';\r\n\r\nexport default {\r\n  name:'adddrone',\r\n  data(){\r\n\r\n    return{\r\n      user:Cookie.get('user') ? JSON.parse(Cookie.get('user')) :{},\r\n      form:{ cover:''},\r\n      categories:[],\r\n      rules:{\r\n        name:[\r\n          {required:true, message:'请输入无人机名称',trigger:'blur'},\r\n        ],\r\n        droneNo:[\r\n          {required:true, message:'请输入无人机标准码',trigger:'blur'},\r\n        ],\r\n        score:[\r\n          {required:true, message:'请输入积分',trigger:'blur'},\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n    request.get('/category/tree').then(res =>{\r\n      this.categories =res.data\r\n    })\r\n  },\r\n  methods:{\r\n    handleCoverSuccess(res){\r\n      if(res.code === '200'){\r\n        // this.$set(this.form,'cover',res.data)\r\n        this.form.cover=res.data\r\n      }\r\n    },\r\n    save(){\r\n      this.$refs['ruleForm'].validate((valid) => {\r\n        if(valid){\r\n          request.post('/drone/save',this.form).then(res =>{\r\n            console.log(this.form)\r\n            if(res.code ==='200'){\r\n              this.$notify.success('新增成功')\r\n              this.$refs['ruleForm'].resetFields()\r\n            }else{\r\n              this.$notify.error(res.msg)\r\n\r\n            }\r\n          })\r\n\r\n        }\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>"],"mappings":"AAmEA,OAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IAEA;MACAC,IAAA,EAAAC,MAAA,CAAAC,GAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,MAAA,CAAAC,GAAA;MACAG,IAAA;QAAAC,KAAA;MAAA;MACAC,UAAA;MACAC,KAAA;QACAV,IAAA,GACA;UAAAW,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,OAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAE,KAAA,GACA;UAAAJ,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACAjB,OAAA,CAAAK,GAAA,mBAAAa,IAAA,CAAAC,GAAA;MACA,KAAAT,UAAA,GAAAS,GAAA,CAAAjB,IAAA;IACA;EACA;EACAkB,OAAA;IACAC,mBAAAF,GAAA;MACA,IAAAA,GAAA,CAAAG,IAAA;QACA;QACA,KAAAd,IAAA,CAAAC,KAAA,GAAAU,GAAA,CAAAjB,IAAA;MACA;IACA;IACAqB,KAAA;MACA,KAAAC,KAAA,aAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA1B,OAAA,CAAA2B,IAAA,qBAAAnB,IAAA,EAAAU,IAAA,CAAAC,GAAA;YACAS,OAAA,CAAAC,GAAA,MAAArB,IAAA;YACA,IAAAW,GAAA,CAAAG,IAAA;cACA,KAAAQ,OAAA,CAAAC,OAAA;cACA,KAAAP,KAAA,aAAAQ,WAAA;YACA;cACA,KAAAF,OAAA,CAAAG,KAAA,CAAAd,GAAA,CAAAe,GAAA;YAEA;UACA;QAEA;MACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}