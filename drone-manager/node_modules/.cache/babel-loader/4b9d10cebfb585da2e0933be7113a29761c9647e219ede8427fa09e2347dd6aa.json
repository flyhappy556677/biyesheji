{"ast":null,"code":"export default {\n  name: 'Maintenancelist',\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      tableData: [],\n      total: 0,\n      cover: '',\n      pageNum: 1,\n      pageSize: 8\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/maintenance/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData.id = res.data.records.id;\n        this.total = res.data.total;\n      });\n    },\n    resert() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        droneNo: ''\n      };\n      this.load();\n    },\n    handelCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/operate/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","localStorage","getItem","JSON","parse","tableData","total","cover","pageNum","pageSize","created","load","methods","request","get","params","then","res","console","log","id","records","resert","droneNo","handelCurrentChange","del","delete","code","$notify","success","error","msg"],"sources":["src/views/Maintenancelist.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <div style=\"padding: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"nickname\"></el-input>\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button class=\"ml-5\" type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button  type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我在想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定删除吗？\"\r\n          @confirm= \"delBatch\"\r\n      >\r\n        <el-button  type=\"danger\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n\r\n      <el-upload action=\"http://localhost:9090/user/import\" :show-file-list=\"false\"  accept=\"xlsx\" :on-success=\"handleImportSuccess\" style=\"display: inline-block\">\r\n        <el-button  type=\"primary\" class=\"ml-5\">导入<i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n\r\n      <el-button  type=\"primary\" class=\"ml-5\" @click=\"exp\">导出<i class=\"el-icon-top\"></i></el-button>\r\n    </div>\r\n\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"role\" label=\"角色\"></el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"hanleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我在想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[8, 10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n\r\n    </div>\r\n\r\n\r\n    <el-dialog title=\"用户信息\" :visible.sync=\"outerVisible\" width=\"30%\">\r\n      <el-form label-width=\"80px\" size=\"smail\">\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"角色\">\r\n          <el-select clearable v-model=\"form.role\" placeholder=\"请选择角色\" style=\"width: 100%\">\r\n            <el-option v-for=\"item in roles\" :key=\"item.name\" :label=\"item.name\" :value=\"item.flag\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"昵称\">\r\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\">\r\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"outerVisible = false\">取 消</el-button>\r\n        <el-button @click=\"save\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nexport default{\r\n  name:'Maintenancelist',\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      tableData: [],\r\n      total:0,\r\n      cover:'',\r\n      pageNum:1,\r\n      pageSize:8,\r\n    }\r\n  },\r\n  created(){\r\n    this.load()\r\n\r\n  },\r\n  methods:{\r\n    load(){\r\n        this.request.get(\"/maintenance/page\",{\r\n          params:{\r\n            pageNum:this.pageNum,\r\n            pageSize:this.pageSize,\r\n\r\n          }\r\n        })\r\n            .then(res =>{\r\n              console.log(res)\r\n              this.tableData.id =res.data.records.id\r\n\r\n              this.total=res.data.total\r\n            })\r\n    },\r\n\r\n    resert(){\r\n      this.params={\r\n        pageNum:1,\r\n        pageSize:10,\r\n        name:'',\r\n        droneNo:''\r\n      }\r\n      this.load()\r\n    },\r\n\r\n    handelCurrentChange(pageNum){\r\n      console.log(pageNum)\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/operate/delete/\"+id).then(res =>{\r\n        if(res.code ==='200'){\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n\r\n\r\n  },\r\n}\r\n\r\n\r\n</script>"],"mappings":"AAgHA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,SAAA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EAEA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAP,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA;QAEA;MACA,GACAO,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAZ,SAAA,CAAAe,EAAA,GAAAH,GAAA,CAAAlB,IAAA,CAAAsB,OAAA,CAAAD,EAAA;QAEA,KAAAd,KAAA,GAAAW,GAAA,CAAAlB,IAAA,CAAAO,KAAA;MACA;IACA;IAEAgB,OAAA;MACA,KAAAP,MAAA;QACAP,OAAA;QACAC,QAAA;QACAX,IAAA;QACAyB,OAAA;MACA;MACA,KAAAZ,IAAA;IACA;IAEAa,oBAAAhB,OAAA;MACAU,OAAA,CAAAC,GAAA,CAAAX,OAAA;MACA,KAAAO,MAAA,CAAAP,OAAA,GAAAA,OAAA;MACA,KAAAG,IAAA;IACA;IACAc,IAAAL,EAAA;MACAP,OAAA,CAAAa,MAAA,sBAAAN,EAAA,EAAAJ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAU,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;UACA,KAAAlB,IAAA;QACA;UACA,KAAAiB,OAAA,CAAAE,KAAA,CAAAb,GAAA,CAAAc,GAAA;QACA;MACA;IACA;EAIA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}