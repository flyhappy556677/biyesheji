{"ast":null,"code":"import request from '@/utils/request';\nimport Cookie from 'js-cookie';\nexport default {\n  name: 'dronelist',\n  data() {\n    return {\n      user: Cookie.get('user') ? JSON.parse(Cookie.get('user')) : {},\n      name: 'drone',\n      tabledata: [],\n      total: 0,\n      cover: '',\n      params: {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        droneNo: '',\n        cover: ''\n      }\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      // fetch('http://localhost:9090/drone/list').then(res =>res.json()).then(res =>{\n      //     console.log(res)\n      //     this.tabledata=res\n      // })\n      request.get('/drone/page', {\n        params: this.params\n      }).then(res => {\n        if (res.code === '200') {\n          this.tabledata = res.data.list;\n          this.total = res.data.total;\n        }\n      });\n    },\n    resert() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        droneNo: ''\n      };\n      this.load();\n    },\n    handelCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/drone/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    },\n    save() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          //给二级无人机赋值pid\n          this.form.pid = this.pid;\n          request.post('/drone/save', this.form).then(res => {\n            if (res.code === '200') {\n              this.$notify.success(\"新增二级无人机成功\");\n              this.$refs['formRef'].resetFields();\n              this.dialogFormVisible = false;\n              this.load();\n            } else {\n              this.$notify.error(res.msg);\n            }\n          });\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookie","name","data","user","get","JSON","parse","tabledata","total","cover","params","pageNum","pageSize","droneNo","created","load","methods","then","res","code","list","resert","handelCurrentChange","console","log","del","id","delete","$notify","success","error","msg","save","$refs","validate","valid","form","pid","post","resetFields","dialogFormVisible"],"sources":["src/views/drone/DroneList.vue"],"sourcesContent":["\r\n<template>\r\n  <div>\r\n    <!-- 搜索表单 -->\r\n    <div>\r\n      <el-input style=\"width: 240px;\" placeholder=\"请输入无人机名称\" v-model=\"params.name\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left:5px\" placeholder=\"请输入无人机编号\" v-model=\"params.droneNo\"></el-input>\r\n      <el-button @click=\"load\">搜索</el-button>\r\n      <el-button @click=\"resert\">重置</el-button>\r\n    </div>\r\n\r\n\r\n    <el-table :data=\"tabledata\" style=\"width: 100%\" stripe row-key=\"id\" default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"无人机名称\"></el-table-column>\r\n      <el-table-column prop=\"description\" label=\"描述\"></el-table-column>\r\n      <el-table-column prop=\"company\" label=\"创造公司\" ></el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"创造时间\"></el-table-column>\r\n      <el-table-column prop=\"category\" label=\"分类\"></el-table-column>\r\n      <el-table-column prop=\"droneNo\" label=\"无人机编号\"></el-table-column>\r\n      <el-table-column prop=\"score\" label=\"所能极限飞行距离\"></el-table-column>\r\n      <el-table-column prop=\"cover\" label=\"图片\">\r\n        <template slot-scope=\"scope\">\r\n          <el-image :src=\"scope.row.cover\" :preview-src-list=\"[scope.row.cover]\" ></el-image>\r\n        </template>\r\n      </el-table-column>\r\n\r\n\r\n      <el-table-column label=\"操作\" width=\"320\">\r\n        <template v-slot=\"scope\">\r\n\r\n          <el-button type=\"primary\" size=\"7\" @click=\"$router.push('/orderEditdrone?id='+scope.row.id)\">编辑</el-button>\r\n\r\n          <el-popconfirm title=\"这是一段内容确定删除吗？\" @confirm=\"del(scope.row.id)\">\r\n            <el-button type=\"danger\" slot=\"reference\">删除</el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <div>\r\n      <el-pagination background layout=\"prev,pager,next\"\r\n                     :total=\"total\"\r\n                     :current-page=\"params.pageNum\"\r\n                     :page-size=\"params.pageSize\"\r\n                     @current-change=\"handelCurrentChange\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\nimport request from '@/utils/request';\r\nimport Cookie from 'js-cookie';\r\n\r\nexport default{\r\n  name:'dronelist',\r\n  data(){\r\n    return{\r\n      user:Cookie.get('user') ? JSON.parse(Cookie.get('user')) :{},\r\n      name:'drone',\r\n      tabledata:[  ],\r\n      total:0,\r\n      cover:'',\r\n      params:{\r\n        pageNum:1,\r\n        pageSize:10,\r\n        name:'',\r\n        droneNo:'',\r\n        cover:''\r\n      },\r\n    }\r\n  },\r\n  created(){\r\n    this.load()\r\n\r\n  },\r\n  methods:{\r\n    load(){\r\n      // fetch('http://localhost:9090/drone/list').then(res =>res.json()).then(res =>{\r\n      //     console.log(res)\r\n      //     this.tabledata=res\r\n      // })\r\n      request.get('/drone/page',{params:this.params}).then(res =>{\r\n        if(res.code==='200'){\r\n          this.tabledata=res.data.list\r\n          this.total=res.data.total\r\n        }\r\n      })\r\n    },\r\n\r\n    resert(){\r\n      this.params={\r\n        pageNum:1,\r\n        pageSize:10,\r\n        name:'',\r\n        droneNo:''\r\n      }\r\n      this.load()\r\n    },\r\n\r\n    handelCurrentChange(pageNum){\r\n      console.log(pageNum)\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/drone/delete/\"+id).then(res =>{\r\n        if(res.code ==='200'){\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n    save(){\r\n      this.$refs['formRef'].validate((valid) =>{\r\n        if(valid){\r\n          //给二级无人机赋值pid\r\n          this.form.pid=this.pid\r\n          request.post('/drone/save',this.form).then(res =>{\r\n            if(res.code === '200')\r\n            {\r\n              this.$notify.success(\"新增二级无人机成功\")\r\n              this.$refs['formRef'].resetFields()\r\n              this.dialogFormVisible = false\r\n              this.load()\r\n            }\r\n            else{\r\n              this.$notify.error(res.msg)\r\n            }\r\n          })\r\n        }\r\n\r\n      } )\r\n    },\r\n  }\r\n}\r\n\r\n</script>"],"mappings":"AAsDA,OAAAA,OAAA;AACA,OAAAC,MAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAH,MAAA,CAAAI,GAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAN,MAAA,CAAAI,GAAA;MACAH,IAAA;MACAM,SAAA;MACAC,KAAA;MACAC,KAAA;MACAC,MAAA;QACAC,OAAA;QACAC,QAAA;QACAX,IAAA;QACAY,OAAA;QACAJ,KAAA;MACA;IACA;EACA;EACAK,QAAA;IACA,KAAAC,IAAA;EAEA;EACAC,OAAA;IACAD,KAAA;MACA;MACA;MACA;MACA;MACAhB,OAAA,CAAAK,GAAA;QAAAM,MAAA,OAAAA;MAAA,GAAAO,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAZ,SAAA,GAAAW,GAAA,CAAAhB,IAAA,CAAAkB,IAAA;UACA,KAAAZ,KAAA,GAAAU,GAAA,CAAAhB,IAAA,CAAAM,KAAA;QACA;MACA;IACA;IAEAa,OAAA;MACA,KAAAX,MAAA;QACAC,OAAA;QACAC,QAAA;QACAX,IAAA;QACAY,OAAA;MACA;MACA,KAAAE,IAAA;IACA;IAEAO,oBAAAX,OAAA;MACAY,OAAA,CAAAC,GAAA,CAAAb,OAAA;MACA,KAAAD,MAAA,CAAAC,OAAA,GAAAA,OAAA;MACA,KAAAI,IAAA;IACA;IACAU,IAAAC,EAAA;MACA3B,OAAA,CAAA4B,MAAA,oBAAAD,EAAA,EAAAT,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAS,OAAA,CAAAC,OAAA;UACA,KAAAd,IAAA;QACA;UACA,KAAAa,OAAA,CAAAE,KAAA,CAAAZ,GAAA,CAAAa,GAAA;QACA;MACA;IACA;IAEAC,KAAA;MACA,KAAAC,KAAA,YAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA;UAC