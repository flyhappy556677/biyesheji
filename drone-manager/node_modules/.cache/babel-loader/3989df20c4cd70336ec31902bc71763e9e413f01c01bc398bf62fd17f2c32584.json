{"ast":null,"code":"export default {\n  name: \"Drone\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 8,\n      droneName: \"\",\n      form: {},\n      multipleSelection: [],\n      outerVisible: false,\n      roles: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/buyorder/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          droneName: this.droneName,\n          buyorderId: this.$route.query.id\n        }\n      }).then(res => {\n        console.log(res.data);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    handleAdd() {\n      this.outerVisible = true, this.form = {};\n    },\n    del(id) {\n      this.request.delete(\"/buyorder/\" + id).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    save() {\n      console.log(this.form);\n      this.request.post(\"/buyorder\", this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"保存成功\");\n          this.outerVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    reset() {\n      this.name = '', this.droneName = '', this.load();\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); //变为集合数组\n\n      this.request.post(\"/buyorder/del/batch\", ids).then(res => {\n        if (res.data) {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    handleAvatarSuccess(res) {\n      this.form.cover = res;\n    },\n    handleImportSuccess() {\n      this.$message.success(\"导入成功\");\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","droneName","form","multipleSelection","outerVisible","roles","created","load","methods","request","get","params","buyorderId","$route","query","id","then","res","console","log","records","handleAdd","del","delete","$message","success","error","save","post","reset","handleSelectionChange","val","delBatch","ids","map","v","handleSizeChange","handleCurrentChange","handleAvatarSuccess","cover","handleImportSuccess"],"sources":["src/views/Buyorder.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <div style=\"padding: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入无人机名称名称\" suffix-icon=\"el-icon-search\" v-model=\"droneName\"></el-input>\r\n\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button class=\"ml-5\" type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button  type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我在想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定删除吗？\"\r\n          @confirm= \"delBatch\"\r\n      >\r\n        <el-button  type=\"danger\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n\r\n    </div>\r\n\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"droneName\" label=\"无人机名称\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"description\" label=\"描述\"></el-table-column>\r\n      <el-table-column prop=\"company\" label=\"公司\" width=\"120\"></el-table-column>\r\n      <el-table-column prop=\"category\" label=\"种类\"></el-table-column>\r\n      <el-table-column prop=\"droneNo\" label=\"无人机编号\"></el-table-column>\r\n      <el-table-column prop=\"score\" label=\"可运行公里数\"></el-table-column>\r\n      <el-table-column prop=\"usescore\" label=\"还剩运行公里数\" ></el-table-column>\r\n      <el-table-column prop=\"note\" label=\"目前状态\">\r\n        <template v-slot=\"scope\">\r\n          <el-tag type=\"success\" v-if=\"scope.row.note === '正常状态'\">{{ scope.row.note }}</el-tag>\r\n          <el-tag type=\"warning\" v-if=\"scope.row.note === '需要保养'\">{{ scope.row.note }}</el-tag>\r\n          <el-tag type=\"primary\" v-if=\"scope.row.note === '正在保养中'\">{{ scope.row.note }}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"cover\" label=\"图片\">\r\n        <template slot-scope=\"scope\">\r\n          <el-image :src=\"scope.row.cover\" :preview-src-list=\"[scope.row.cover]\"  style=\"width: 70px; height: 70px\"></el-image>\r\n        </template>\r\n      </el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"300px\"  align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <div v-if=\"scope.row.status\">\r\n\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我在想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n\r\n          </el-popconfirm>\r\n            \r\n          <el-button type=\"primary\"  @click=\"$router.push('/operatedrone?id='+scope.row.id)\" style=\"margin-left: 5px\">进入操作无人机页面</el-button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[8, 10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n<script >\r\nexport  default {\r\n  name:\"Drone\",\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n      total:0,\r\n      pageNum:1,\r\n      pageSize:8,\r\n      droneName:\"\",\r\n      form:{},\r\n      multipleSelection:[],\r\n      outerVisible: false,\r\n      roles:[]\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      this.request.get(\"/buyorder/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          droneName:this.droneName,\r\n          buyorderId:this.$route.query.id\r\n        }\r\n      })\r\n          .then(res =>{\r\n            console.log(res.data)\r\n            this.tableData =res.data.records\r\n            this.total=res.data.total\r\n          })\r\n\r\n    },\r\n    handleAdd(){\r\n      this.outerVisible =true,\r\n          this.form={}\r\n    },\r\n    del(id){\r\n      this.request.delete(\"/buyorder/\" +id).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    save(){\r\n      console.log(this.form)\r\n      this.request.post(\"/buyorder\",this.form).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"保存成功\")\r\n          this.outerVisible=false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.name ='',\r\n          this.droneName='',\r\n          this.load()\r\n\r\n    },\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection=val\r\n    },\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v => v.id)//变为集合数组\r\n\r\n      this.request.post(\"/buyorder/del/batch\",ids).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize\r\n      this.load()\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load()\r\n    },\r\n\r\n    handleAvatarSuccess(res) {\r\n      this.form.cover = res\r\n    },\r\n    handleImportSuccess(){\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.headerBg{\r\n  background-color: #eee!important;\r\n}\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 138px;\r\n  height: 138px;\r\n  line-height: 138px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 138px;\r\n  height: 138px;\r\n  display: block;\r\n}\r\n</style>"],"mappings":"AA2FA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,SAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,YA