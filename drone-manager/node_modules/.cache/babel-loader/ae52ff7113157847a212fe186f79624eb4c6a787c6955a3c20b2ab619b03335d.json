{"ast":null,"code":"import Aside from \"@/components/Aside.vue\";\nimport Header from \"@/components/Header.vue\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      username: \"\",\n      nickname: \"\",\n      address: \"\",\n      sollapseBthClass: 'el-icon-s-fold',\n      multipleSelection: [],\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShot: true,\n      headerBg: 'headerBg',\n      outerVisible: false,\n      form: {}\n    };\n  },\n  created() {\n    this.load();\n  },\n  components: {\n    Aside,\n    Header\n  },\n  methods: {\n    collapse() {\n      //点击收缩触发\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse) {\n        this.sideWidth = 64;\n        this.collapseBtnClass = 'el-icon-s-unflod';\n        this.logoTextShot = false;\n      } else {\n        this.sideWidth = 200;\n        this.collapseBtnClass = 'el-icon-s-flod';\n        this.logoTextShot = true;\n      }\n    },\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          address: this.address,\n          nickname: this.nickname\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.records;\n        this.total = res.total;\n      });\n    },\n    hanleEdit(row) {\n      this.form = row;\n      this.outerVisible = true;\n    },\n    handleAdd() {\n      this.outerVisible = true, this.form = {};\n    },\n    del(id) {\n      this.request.delete(\"/user/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.outerVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    reset() {\n      this.username = '', this.address = '', this.nickname = '', this.load();\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); //变为集合数组\n\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res) {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["Aside","Header","data","tableData","total","pageNum","pageSize","username","nickname","address","sollapseBthClass","multipleSelection","isCollapse","sideWidth","logoTextShot","headerBg","outerVisible","form","created","load","components","methods","collapse","collapseBtnClass","request","get","params","then","res","console","log","records","hanleEdit","row","handleAdd","del","id","delete","$message","success","error","save","post","reset","handleSelectionChange","val","delBatch","ids","map","v","handleSizeChange","handleCurrentChange"],"sources":["src/views/Manage.vue"],"sourcesContent":["<template>\n\n      <el-container  style=\"min-height: 100vh\">\n        <el-aside :width=\"sideWidth +'px'\"  style=\"background-color: rgb(238,241,246) ; box-shadow: 2px 0 6px rgb( 0 21 41  /35%) \">\n          <Aside  :is-collapse=\"isCollapse\" :logoTextShot=\"logoTextShot\" />\n        </el-aside>\n\n        <el-container>\n          <el-header style=\" font-size: 12px ; border-bottom: 1px solid #ccc; line-height: 60px; display: flex \" >\n            <Header :sollapseBthClass=\"sollapseBthClass \"  :collapse=\"collapse\"/>\n          </el-header>\n\n          <el-main>\n\n            <div style=\"padding: 10px 0\">\n              <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\n              <el-input style=\"width: 200px\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"nickname\"></el-input>\n              <el-input style=\"width: 200px\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>\n              <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n              <el-button class=\"ml-5\" type=\"warning\" @click=\"reset\">重置</el-button>\n            </div>\n\n            <div style=\"margin: 10px 0\">\n              <el-button  type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\n\n              <el-popconfirm\n                  class=\"ml-5\"\n                  confirm-button-text='确定'\n                  cancel-button-text='我在想想'\n                  icon=\"el-icon-info\"\n                  icon-color=\"red\"\n                  title=\"您确定删除吗？\"\n                  @confirm= \"delBatch\"\n              >\n                <el-button  type=\"danger\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\n              </el-popconfirm>\n\n              <el-button  type=\"primary\" class=\"ml-5\">导入<i class=\"el-icon-bottom\"></i></el-button>\n              <el-button  type=\"primary\" class=\"ml-5\">导出<i class=\"el-icon-top\"></i></el-button>\n            </div>\n\n\n            <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\n              <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n              <el-table-column prop=\"id\" label=\"ID\" width=\"140\"></el-table-column>\n              <el-table-column prop=\"username\" label=\"用户名\" width=\"140\"></el-table-column>\n              <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\"></el-table-column>\n              <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\n              <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\n              <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n              <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n                <template slot-scope=\"scope\">\n                  <el-button type=\"success\" @click=\"hanleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\n\n                  <el-popconfirm\n                      class=\"ml-5\"\n                      confirm-button-text='确定'\n                      cancel-button-text='我在想想'\n                      icon=\"el-icon-info\"\n                      icon-color=\"red\"\n                      title=\"您确定删除吗？\"\n                      @confirm=\"del(scope.row.id)\"\n                  >\n                    <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n                  </el-popconfirm>\n\n                </template>\n              </el-table-column>\n\n            </el-table>\n            <div style=\"padding: 10px 0\">\n              <el-pagination\n                  @size-change=\"handleSizeChange\"\n                  @current-change=\"handleCurrentChange\"\n                  :current-page=\"pageNum\"\n                  :page-sizes=\"[2, 10, 15, 20]\"\n                  :page-size=\"pageSize\"\n                  layout=\"total, sizes, prev, pager, next, jumper\"\n                  :total=\"total\">\n              </el-pagination>\n\n            </div>\n\n\n            <el-dialog title=\"用户信息\" :visible.sync=\"outerVisible\" width=\"30%\">\n                <el-form label-width=\"80px\" size=\"smail\">\n                  <el-form-item label=\"用户名\">\n                    <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"昵称\">\n                    <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"邮箱\">\n                    <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"电话\">\n                    <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"地址\">\n                    <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                </el-form>\n              <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"outerVisible = false\">取 消</el-button>\n                <el-button @click=\"save\">提交</el-button>\n              </div>\n            </el-dialog>\n\n          </el-main>\n        </el-container>\n      </el-container>\n\n</template>\n\n<script>\nimport Aside from \"@/components/Aside.vue\";\nimport Header from \"@/components/Header.vue\";\n\nexport default {\n  data() {\n\n    return {\n      tableData: [],\n      total:0,\n      pageNum:1,\n      pageSize:2,\n      username:\"\",\n      nickname:\"\",\n      address:\"\",\n      sollapseBthClass:'el-icon-s-fold',\n      multipleSelection:[],\n      isCollapse:false,\n      sideWidth:200,\n      logoTextShot:true,\n      headerBg:'headerBg',\n      outerVisible:false,\n      form:{}\n    }\n  },\n  created() {\n   this.load()\n  },\n  components:{Aside,Header},\n  methods:{\n    collapse(){  //点击收缩触发\n      this.isCollapse = !this.isCollapse\n      if(this.isCollapse){\n        this.sideWidth=64\n        this.collapseBtnClass ='el-icon-s-unflod'\n        this.logoTextShot=false\n      }else{\n        this.sideWidth=200\n        this.collapseBtnClass ='el-icon-s-flod'\n        this.logoTextShot=true\n      }\n\n    },\n    load(){\n      this.request.get(\"/user/page\",{\n        params:{\n          pageNum:this.pageNum,\n          pageSize:this.pageSize,\n          username:this.username,\n          address:this.address,\n          nickname:this.nickname\n        }\n      })\n          .then(res =>{\n            console.log(res)\n\n            this.tableData =res.records\n            this.total=res.total\n\n          })\n\n    },\n    hanleEdit(row){\n      this.form=row\n      this.outerVisible=true\n    },\n    handleAdd(){\n      this.outerVisible =true,\n      this.form={}\n    },\n    del(id){\n      this.request.delete(\"/user/\" +id).then(res =>{\n        if(res){\n          this.$message.success(\"删除成功\")\n          this.load()\n        }else{\n          this.$message.error(\"删除失败\")\n        }\n      })\n    },\n    save(){\n      this.request.post(\"/user\",this.form).then(res =>{\n        if(res){\n          this.$message.success(\"保存成功\")\n          this.outerVisible=false\n          this.load()\n        }else{\n          this.$message.error(\"保存失败\")\n        }\n      })\n    },\n    reset(){\n      this.username ='',\n      this.address='',\n      this.nickname='',\n          this.load()\n\n    },\n    handleSelectionChange(val){\n      console.log(val)\n      this.multipleSelection=val\n    },\n    delBatch(){\n      let ids = this.multipleSelection.map(v => v.id)//变为集合数组\n\n      this.request.post(\"/user/del/batch\",ids).then(res =>{\n        if(res){\n          this.$message.success(\"批量删除成功\")\n          this.load()\n        }else{\n          this.$message.error(\"批量删除失败\")\n        }\n      })\n    },\n    handleSizeChange(pageSize){\n      this.pageSize=pageSize\n      this.load()\n\n    },\n    handleCurrentChange(pageNum){\n      this.pageNum=pageNum\n      this.load()\n    }\n  }\n};\n</script>\n<style>\n.headerBg{\n  background-color: #eee!important;\n}\n</style>"],"mappings":"AAmHA,OAAAA,KAA