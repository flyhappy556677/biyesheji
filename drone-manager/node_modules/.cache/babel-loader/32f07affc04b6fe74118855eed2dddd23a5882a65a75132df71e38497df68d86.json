{"ast":null,"code":"export default {\n  name: \"File\",\n  data() {\n    return {\n      tableData: [],\n      name: '',\n      multipleSelection: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/file/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    del(id) {\n      this.request.delete(\"/file/\" + id).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    reset() {\n      this.name = '', this.load();\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); //变为集合数组\n      this.request.post(\"/file/del/batch\", ids).then(res => {\n        if (res.data) {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    handleFileUploadSuccess(res) {\n      console.log(res);\n    },\n    download(url) {\n      window.open(url);\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","multipleSelection","pageNum","pageSize","total","created","load","methods","request","get","params","then","res","records","del","id","delete","$message","success","error","reset","handleSelectionChange","val","console","log","delBatch","ids","map","v","post","handleSizeChange","handleCurrentChange","handleFileUploadSuccess","download","url","window","open"],"sources":["src/views/File.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n\r\n      <el-upload action=\"http://localhost:9090/file/upload\" :show-file-list=\"false\"  accept=\"xlsx\" :on-success=\"handleFileUploadSuccess\" style=\"display: inline-block\">\r\n        <el-button  type=\"primary\" @click=\"handleAdd\">上传文件<i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n      </el-upload>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我在想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定删除吗？\"\r\n          @confirm= \"delBatch\"\r\n      >\r\n        <el-button  type=\"danger\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"文件名称\" ></el-table-column>\r\n      <el-table-column prop=\"type\" label=\"文件类型\" ></el-table-column>\r\n      <el-table-column prop=\"size\" label=\"文件大小（kb）\"></el-table-column>\r\n      <el-table-column label=\"下载\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"primary\" @click=\"download(scope.row.url)\"> 下载</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"启用\">\r\n        <template slot-scope=\"scope\">\r\n            <el-switch v-model=\"scope.row.enable\" active-color=\"#13ce66\" inactive-color=\"#ccc\" @change=\"changeEnable(scope.row)\"> </el-switch>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我在想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[8, 10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script >\r\nexport default {\r\n  name:\"File\",\r\n  data(){\r\n    return{\r\n      tableData:[],\r\n      name: '',\r\n      multipleSelection:[],\r\n      pageNum:1,\r\n      pageSize:10,\r\n      total:0\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      this.request.get(\"/file/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          name:this.name,\r\n\r\n        }\r\n      })\r\n          .then(res =>{\r\n            this.tableData =res.data.records\r\n            this.total=res.data.total\r\n\r\n          })\r\n\r\n    },\r\n\r\n    del(id){\r\n      this.request.delete(\"/file/\" +id).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n\r\n    reset(){\r\n      this.name ='',\r\n      this.load()\r\n\r\n    },\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection=val\r\n    },\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v => v.id)//变为集合数组\r\n      this.request.post(\"/file/del/batch\",ids).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize\r\n      this.load()\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res){\r\n      console.log(res)\r\n    },\r\n    download(url){\r\n      window.open(url)\r\n    }\r\n\r\n\r\n  }\r\n}\r\n</script>"],"mappings":"AAyEA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAF,IAAA;MACAG,iBAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAR,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAL,IAAA,OAAAA;QAEA;MACA,GACAa,IAAA,CAAAC,GAAA;QACA,KAAAZ,SAAA,GAAAY,GAAA,CAAAb,IAAA,CAAAc,OAAA;QACA,KAAAT,KAAA,GAAAQ,GAAA,CAAAb,IAAA,CAAAK,KAAA;MAEA;IAEA;IAEAU,IAAAC,EAAA;MACA,KAAAP,OAAA,CAAAQ,MAAA,YAAAD,EAAA,EAAAJ,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAb,IAAA;UACA,KAAAkB,QAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IAEAC,MAAA;MACA,KAAAtB,IAAA,OACA,KAAAQ,IAAA;IAEA;IACAe,sBAAAC,GAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;MACA,KAAArB,iBAAA,GAAAqB,GAAA;IACA;IACAG,SAAA;MACA,IAAAC,GAAA,QAAAzB,iBAAA,CAAA0B,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAb,EAAA;MACA,KAAAP,OAAA,CAAAqB,IAAA,oBAAAH,GAAA,EAAAf,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAb,IAAA;UACA,KAAAkB,QAAA,CAAAC,OAAA;UACA,KAAAZ,IAAA;QACA;UACA,KAAAW,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAW,iBAAA3B,QAAA;MACA,KAAAA,QAAA,GAAAA,QAAA;MACA,KAAAG,IAAA;IAEA;IACA