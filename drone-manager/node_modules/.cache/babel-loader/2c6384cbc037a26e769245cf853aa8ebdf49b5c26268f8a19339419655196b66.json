{"ast":null,"code":"export default {\n  name: 'MATman',\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      form: {},\n      orders: [],\n      rules: {},\n      // 添加 rules 属性\n      orederid: null\n    };\n  },\n  created() {\n    const id = this.$route.query.id;\n    request.get(\"/maintenance/\" + id).then(res => {\n      this.form = res.data;\n      console.log(res.data);\n      const orderId = res.data.orderId;\n      request.get('/order/getById/' + orderId).then(res => {\n        this.form.score = res.data.score;\n        console.log(this.form);\n      });\n    });\n    const user = Cookie.get('user') ? JSON.parse(Cookie.get('user')) : {};\n    console.log(user);\n    this.maintenanceId = user.id;\n    this.maintenanceName = user.username;\n  },\n  Loading() {},\n  methods: {\n    save() {\n      this.form.maintenanceId = this.maintenanceId;\n      this.form.maintenanceName = this.maintenanceName;\n      request.put('/maintenance/update', this.form).then(res => {\n        console.log(this.form);\n        if (res.code === '200') {\n          this.$notify.success('更新成功');\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n      console.log(this.form);\n      this.form.status = false;\n      request.put('/order/statusupdate', {\n        ...this.form,\n        id: this.form.orderId\n      }).then(res => {\n        if (res.code === '200') {\n          console.log(this.form.status);\n          this.$notify.success('更新成功');\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","localStorage","getItem","JSON","parse","form","orders","rules","orederid","created","id","$route","query","request","get","then","res","console","log","orderId","score","Cookie","maintenanceId","maintenanceName","username","Loading","methods","save","put","code","$notify","success","error","msg","status"],"sources":["src/views/MATman.vue"],"sourcesContent":["<template>\r\n  <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" :inline=\"true\" style=\"width: 70%\" label-width=\"100px\">\r\n\r\n    <el-form-item label=\"操作用户\" prop=\"userName\">\r\n      <el-input v-model=\"form.userName\" disabled placeholder=\"请输入无人机名称\"></el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"无人机名字\" prop=\"droneName\">\r\n      <el-input v-model=\"form.droneName\" disabled placeholder=\"请输入无人机名字\"></el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"操作地址\" prop=\"address\">\r\n      <el-input style=\"width:400px\" disabled v-model=\"form.address\"  placeholder=\"请输入描述\"></el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"预留手机号码\" prop=\"phone\">\r\n      <el-input style=\"width:400px\" disabled v-model=\"form.phone\"  placeholder=\"请输入描述\"></el-input>\r\n    </el-form-item>\r\n\r\n\r\n    <el-form-item label=\"保养提交时间\" prop=\"createtime\">\r\n      <el-date-picker disabled\r\n                      v-model=\"form.createtime\"\r\n                      type=\"date\"\r\n                      value-format=\"yyyy-MM-dd\"\r\n                      placeholder=\"保养提交时间\">\r\n      </el-date-picker>\r\n    </el-form-item>\r\n\r\n\r\n    <el-form-item label=\"维修员名字\" prop=\"maintenanceName\">\r\n      <el-input v-model=\"maintenanceName\" disabled placeholder=\"请输入无人机名称\"></el-input>\r\n    </el-form-item>\r\n\r\n\r\n\r\n    <el-form-item label=\"保养完成时间\" prop=\"updatetime\">\r\n      <el-date-picker\r\n          v-model=\"form.updatetime\"\r\n          type=\"date\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          placeholder=\"保养完成时间\">\r\n      </el-date-picker>\r\n    </el-form-item>\r\n\r\n\r\n\r\n    <div style=\"text-align:center; margin-top: 30px;\" >\r\n      <el-button type=\"primary\" @click=\"save\">提交</el-button>\r\n      <el-button type=\"danger\">取消</el-button>\r\n    </div>\r\n\r\n  </el-form>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name:'MATman',\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      form:{},\r\n      orders:[],\r\n      rules: {}  ,// 添加 rules 属性\r\n      orederid:null\r\n    }\r\n  },\r\n\r\n  created(){\r\n    const id = this.$route.query.id;\r\n\r\n    request.get(\"/maintenance/\"+id).then(res =>{\r\n      this.form =res.data\r\n      console.log(res.data)\r\n      const orderId=res.data.orderId\r\n\r\n      request.get('/order/getById/'+orderId).then(res =>{\r\n        this.form.score =res.data.score\r\n        console.log(this.form)\r\n\r\n      })\r\n\r\n    })\r\n    const user = Cookie.get('user') ? JSON.parse(Cookie.get('user')) : {};\r\n    console.log(user)\r\n    this.maintenanceId = user.id;\r\n    this.maintenanceName=user.username;\r\n  },\r\n  Loading(){\r\n\r\n  },\r\n\r\n  methods:{\r\n\r\n    save(){\r\n      this.form.maintenanceId=this.maintenanceId\r\n      this.form.maintenanceName=this.maintenanceName\r\n      request.put('/maintenance/update',this.form).then(res =>{\r\n        console.log(this.form)\r\n        if(res.code ==='200'){\r\n          this.$notify.success('更新成功')\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n\r\n        }\r\n      })\r\n      console.log(this.form)\r\n\r\n      this.form.status=false\r\n      request.put('/order/statusupdate',{...this.form,id:this.form.orderId}).then(res =>{\r\n        if(res.code ==='200'){\r\n          console.log(this.form.status)\r\n          this.$notify.success('更新成功')\r\n\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n\r\n        }\r\n      })\r\n\r\n\r\n\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n  \r\n\r\n"],"mappings":"AAyDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,IAAA;MACAC,MAAA;MACAC,KAAA;MAAA;MACAC,QAAA;IACA;EACA;EAEAC,QAAA;IACA,MAAAC,EAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAF,EAAA;IAEAG,OAAA