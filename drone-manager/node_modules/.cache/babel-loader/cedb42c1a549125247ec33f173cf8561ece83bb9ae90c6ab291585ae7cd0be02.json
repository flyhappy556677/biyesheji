{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'Header',\n  props: {\n    sollapseBthClass: String,\n    collapse: Boolean\n  },\n  computed: {\n    currentPathName() {\n      return this.$store.state.currentPathName; //需要监听数据\n    }\n  },\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\n    };\n  },\n  watch: {\n    //监听路由搜索出结果，监听路由变化\n    currentPathName(newVal, oldVal) {\n      console.log(newVal);\n    }\n  },\n  methods: {\n    collapse() {\n      this.$emit(\"asideCollapse\");\n    },\n    logout() {\n      this.$router.push(\"/login\");\n      localStorage.removeItem(\"user\");\n      this.$message.success(\"退出成功\");\n    }\n  }\n};","map":{"version":3,"names":["name","props","sollapseBthClass","String","collapse","Boolean","computed","currentPathName","$store","state","data","user","localStorage","getItem","JSON","parse","watch","newVal","oldVal","console","log","methods","$emit","logout","$router","push","removeItem","$message","success"],"sources":["src/components/Header.vue"],"sourcesContent":["<template>\r\n  <div style=\"font-size: 12px ; line-height: 60px; display: flex \">\r\n    <div style=\"flex: 1; \">\r\n      <span :class=\"sollapseBthClass\" style=\"cursor: pointer; font-size: 22px\" @click=\"collapse\" ></span>\r\n\r\n      <el-breadcrumb separator=\"/\" style=\"display: inline-block; margin-left: 10px\">\r\n        <el-breadcrumb-item :to=\"{path:'/'}\">首页</el-breadcrumb-item>\r\n        <el-breadcrumb-item >{{currentPathName}}</el-breadcrumb-item>\r\n      </el-breadcrumb>\r\n    </div>\r\n\r\n    <el-dropdown style=\" cursor: pointer\" >\r\n      <div style=\"width: 150px\">\r\n        <img :src=\"user.avatarUrl\" alt=\"\"\r\n              style=\"width: 30px; border-radius: 50%; position: relative; top: 10px; right: 5px\">\r\n        <span>{{user.nickname}}</span><i class=\"el-icon—arrow-down\" style=\"margin-right:5px\"></i>\r\n      </div>\r\n\r\n      <el-dropdown-menu slot=\"dropdown\" style=\"width: 100px; text-align: center\">\r\n        <el-dropdown-item>\r\n          <router-link to=\"/person\" style=\"text-decoration: none; color: black\">个人信息</router-link >\r\n        </el-dropdown-item>\r\n        <el-dropdown-item>\r\n          <span to=\"/login\" style=\"text-decoration:  none\" @click=\"logout\">退出</span>\r\n        </el-dropdown-item>\r\n      </el-dropdown-menu>\r\n    </el-dropdown>\r\n\r\n  </div>\r\n</template>\r\n<script >\r\nexport  default {\r\n  name:'Header',\r\n  props:{\r\n    sollapseBthClass:String,\r\n    collapse:Boolean,\r\n\r\n  },\r\n  computed:{\r\n    currentPathName(){\r\n      return this.$store.state.currentPathName;//需要监听数据\r\n    }\r\n  },\r\n  data(){\r\n    return{\r\n      user:localStorage.getItem(\"user\") ?JSON.parse(localStorage.getItem(\"user\")):{}\r\n    }\r\n  },\r\n  watch:{//监听路由搜索出结果，监听路由变化\r\n    currentPathName (newVal,oldVal){\r\n      console.log(newVal)\r\n    }\r\n  },\r\n  methods:{\r\n    collapse(){\r\n      this.$emit(\"asideCollapse\")\r\n    },\r\n    logout(){\r\n      this.$router.push(\"/login\")\r\n      localStorage.removeItem(\"user\")\r\n      this.$message.success(\"退出成功\")\r\n    }\r\n  }\r\n\r\n\r\n}\r\n</script>"],"mappings":";AA+BA;EACAA,IAAA;EACAC,KAAA;IACAC,gBAAA,EAAAC,MAAA;IACAC,QAAA,EAAAC;EAEA;EACAC,QAAA;IACAC,gBAAA;MACA,YAAAC,MAAA,CAAAC,KAAA,CAAAF,eAAA;IACA;EACA;EACAG,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;IACA;EACA;EACAG,KAAA;IAAA;IACAT,gBAAAU,MAAA,EAAAC,MAAA;MACAC,OAAA,CAAAC,GAAA,CAAAH,MAAA;IACA;EACA;EACAI,OAAA;IACAjB,SAAA;MACA,KAAAkB,KAAA;IACA;IACAC,OAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;MACAb,YAAA,CAAAc,UAAA;MACA,KAAAC,QAAA,CAAAC,OAAA;IACA;EACA;AAGA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}