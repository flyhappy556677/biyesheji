{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 2,\n      username: \"\",\n      nickname: \"\",\n      address: \"\",\n      sollapseBthClass: 'el-icon-s-fold',\n      multipleSelection: [],\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShot: true,\n      headerBg: 'headerBg',\n      outerVisible: false,\n      form: {}\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    collapse() {\n      //点击收缩触发\n      this.isCollapse = !this.isCollapse;\n      if (this.isCollapse) {\n        this.sideWidth = 64;\n        this.collapseBtnClass = 'el-icon-s-unflod';\n        this.logoTextShot = false;\n      } else {\n        this.sideWidth = 200;\n        this.collapseBtnClass = 'el-icon-s-flod';\n        this.logoTextShot = true;\n      }\n    },\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          address: this.address,\n          nickname: this.nickname\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.records;\n        this.total = res.total;\n      });\n    },\n    hanleEdit(row) {\n      this.form = row;\n      this.outerVisible = true;\n    },\n    handleAdd() {\n      this.outerVisible = true, this.form = {};\n    },\n    del(id) {\n      request.delete(\"/user/\" + id).then(res => {\n        if (res) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    save() {\n      request.post(\"/user\", this.form).then(res => {\n        if (res) {\n          this.$message.success(\"保存成功\");\n          this.outerVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    reset() {\n      this.username = '', this.address = '', this.nickname = '', this.load();\n    },\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["request","data","tableData","total","pageNum","pageSize","username","nickname","address","sollapseBthClass","multipleSelection","isCollapse","sideWidth","logoTextShot","headerBg","outerVisible","form","created","load","methods","collapse","collapseBtnClass","get","params","then","res","console","log","records","hanleEdit","row","handleAdd","del","id","delete","$message","success","error","save","post","reset","handleSelectionChange","val","handleSizeChange","handleCurrentChange"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n\n      <el-container  style=\"min-height: 100vh\">\n        <el-aside :width=\"sideWidth +'px'\"  style=\"background-color: rgb(238,241,246) ; box-shadow: 2px 0 6px rgb( 0 21 41  /35%) \">\n\n          <el-menu :default-openeds=\"['1', '3']\"\n                   style=\"min-height:100% ; overflow-x: hidden\"\n\n                   background-color=\"rgb(48,65,86)\"\n                   text-color=\"#fff\"\n                   active-text-color=\"#ffd04b\"\n                   :collapse-transition=\"false\"\n                   :collapse=\"isCollapse\"\n          >\n            <div style=\"height: 60px; line-height: 60px; text-align: center\">\n              <img src=\"../assets/logo.png\"  alt=\" \" style=\"width: 20px ; position: relative; top: 5px; margin-right: 5px\">\n              <b style=\"color: white\" v-show=\"logoTextShot\"> 无人机后台管理系统</b>\n            </div>\n            <el-submenu index=\"1\">\n              <template slot=\"title\">\n                <i class=\"el-icon-message\"></i>\n                <span slot=\"title\">导航一</span>\n              </template>\n\n              <el-submenu index=\"1-4\">\n                <template slot=\"title\">选项4</template>\n                <el-menu-item index=\"1-4-1\">选项4-1</el-menu-item>\n              </el-submenu>\n            </el-submenu>\n            <el-submenu index=\"2\">\n              <template slot=\"title\">\n                <i class=\"el-icon-menu\"></i>\n                <span slot=\"title\">导航一</span>\n              </template>\n\n              <el-submenu index=\"2-4\">\n                <template slot=\"title\">选项4</template>\n                <el-menu-item index=\"2-4-1\">选项4-1</el-menu-item>\n              </el-submenu>\n            </el-submenu>\n            <el-submenu index=\"3\">\n              <template slot=\"title\">\n                <i class=\"el-icon-setting\"></i>\n                <span slot=\"title\">导航一</span>\n              </template>\n\n              <el-submenu index=\"3-4\">\n                <template slot=\"title\">选项4</template>\n                <el-menu-item index=\"3-4-1\">选项4-1</el-menu-item>\n              </el-submenu>\n            </el-submenu>\n          </el-menu>\n        </el-aside>\n\n        <el-container>\n          <el-header style=\" font-size: 12px ; border-bottom: 1px solid #ccc; line-height: 60px; display: flex \" >\n            <div style=\"flex: 1; font-size: 22px\">\n              <span :class=\"sollapseBthClass\" style=\"cursor: pointer\" @click=\"collapse\" ></span>\n            </div>\n\n            <el-dropdown style=\"width: 50px; cursor: pointer\" >\n              <span>王小虎</span>\n              <i class=\"el-icon—arrow-down\" style=\"margin-right:5px\"></i>\n\n              <el-dropdown-menu slot=\"dropdown\">\n                <el-dropdown-item>个人信息</el-dropdown-item>\n                <el-dropdown-item>退出</el-dropdown-item>\n              </el-dropdown-menu>\n            </el-dropdown>\n\n          </el-header>\n\n          <el-main>\n\n            <div style=\"padding: 10px 0\">\n              <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\n              <el-input style=\"width: 200px\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"nickname\"></el-input>\n              <el-input style=\"width: 200px\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>\n              <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n              <el-button class=\"ml-5\" type=\"warning\" @click=\"reset\">重置</el-button>\n            </div>\n\n            <div style=\"margin: 10px 0\">\n              <el-button  type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\n              <el-button  type=\"danger\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\n              <el-button  type=\"primary\">导入<i class=\"el-icon-bottom\"></i></el-button>\n              <el-button  type=\"primary\">导出<i class=\"el-icon-top\"></i></el-button>\n            </div>\n\n\n            <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\n              <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n              <el-table-column prop=\"id\" label=\"ID\" width=\"140\"></el-table-column>\n              <el-table-column prop=\"username\" label=\"用户名\" width=\"140\"></el-table-column>\n              <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\"></el-table-column>\n              <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\n              <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\n              <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n              <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n                <template slot-scope=\"scope\">\n                  <el-button type=\"success\" @click=\"hanleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\n\n                  <el-popconfirm\n                      class=\"ml-5\"\n                      confirm-button-text='确定'\n                      cancel-button-text='我在想想'\n                      icon=\"el-icon-info\"\n                      icon-color=\"red\"\n                      title=\"您确定删除吗？\"\n                      @confirm=\"del(scope.row.id)\"\n                  >\n                    <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n                  </el-popconfirm>\n\n                </template>\n              </el-table-column>\n\n            </el-table>\n            <div style=\"padding: 10px 0\">\n              <el-pagination\n                  @size-change=\"handleSizeChange\"\n                  @current-change=\"handleCurrentChange\"\n                  :current-page=\"pageNum\"\n                  :page-sizes=\"[2, 10, 15, 20]\"\n                  :page-size=\"pageSize\"\n                  layout=\"total, sizes, prev, pager, next, jumper\"\n                  :total=\"total\">\n              </el-pagination>\n\n            </div>\n\n\n            <el-dialog title=\"用户信息\" :visible.sync=\"outerVisible\" width=\"30%\">\n                <el-form label-width=\"80px\" size=\"smail\">\n                  <el-form-item label=\"用户名\">\n                    <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"昵称\">\n                    <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"邮箱\">\n                    <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"电话\">\n                    <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                  <el-form-item label=\"地址\">\n                    <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n                  </el-form-item>\n                </el-form>\n              <div slot=\"footer\" class=\"dialog-footer\">\n                <el-button @click=\"outerVisible = false\">取 消</el-button>\n                <el-button @click=\"save\">提交</el-button>\n              </div>\n            </el-dialog>\n\n          </el-main>\n        </el-container>\n      </el-container>\n\n</template>\n\n<script>\nimport request from \"@/utils/request\";\n\nexport default {\n  data() {\n\n    return {\n      tableData: [],\n      total:0,\n      pageNum:1,\n      pageSize:2,\n      username:\"\",\n      nickname:\"\",\n      address:\"\",\n      sollapseBthClass:'el-icon-s-fold',\n      multipleSelection:[],\n      isCollapse:false,\n      sideWidth:200,\n      logoTextShot:true,\n      headerBg:'headerBg',\n      outerVisible:false,\n      form:{}\n    }\n  },\n  created() {\n   this.load()\n  },\n  methods:{\n    collapse(){  //点击收缩触发\n      this.isCollapse = !this.isCollapse\n      if(this.isCollapse){\n        this.sideWidth=64\n        this.collapseBtnClass ='el-icon-s-unflod'\n        this.logoTextShot=false\n      }else{\n        this.sideWidth=200\n        this.collapseBtnClass ='el-icon-s-flod'\n        this.logoTextShot=true\n      }\n\n    },\n    load(){\n      this.request.get(\"/user/page\",{\n        params:{\n          pageNum:this.pageNum,\n          pageSize:this.pageSize,\n          username:this.username,\n          address:this.address,\n          nickname:this.nickname\n        }\n      })\n          .then(res =>{\n            console.log(res)\n\n            this.tableData =res.records\n            this.total=res.total\n\n          })\n\n    },\n    hanleEdit(row){\n      this.form=row\n      this.outerVisible=true\n    },\n    handleAdd(){\n      this.outerVisible =true,\n      this.form={}\n    },\n    del(id){\n      request.delete(\"/user/\" +id).then(res =>{\n        if(res){\n          this.$message.success(\"删除成功\")\n          this.load()\n        }else{\n          this.$message.error(\"保存失败\")\n        }\n      })\n    },\n    save(){\n      request.post(\"/user\",this.form).then(res =>{\n        if(res){\n          this.$message.success(\"保存成功\")\n          this.outerVisible=false\n          this.load()\n        }else{\n          this.$message.error(\"保存失败\")\n        }\n      })\n    },\n    reset(){\n      this.username ='',\n      this.address='',\n      this.nickname='',\n          this.load()\n\n    },\n    handleSelectionChange(val){\n      this.multipleSelection=val\n    },\n    handleSizeChange(pageSize){\n      this.pageSize=pageSize\n      this.load()\n\n    },\n    handleCurrentChange(pageNum){\n      this.pageNum=pageNum\n      this.load()\n    }\n  }\n};\n</script>\n<style>\n.headerBg{\n  background-color: #eee!important;\n}\n</style>"],"mappings":"AAmKA,OAAAA,OAAA;AAEA;EACAC,KAAA;IAEA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA;MACAC,QAAA;MACAC,OAAA;MACAC,gBAAA;MACAC,iBAAA;MACAC,UAAA;MACAC,SAAA;MACAC,YAAA;MACAC,QAAA;MACAC,YAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,SAAA;MAAA;MACA,KAAAT,UAAA,SAAAA,UAAA;MACA,SAAAA,UAAA;QACA,KAAAC,SAAA;QACA,KAAAS,gBAAA;QACA,KAAAR,YAAA;MACA;QACA,KAAAD,SAAA;QACA,KAAAS,gBAAA;QACA,KAAAR,YAAA;MACA;IAEA;IACAK,KAAA;MACA,KAAAlB,OAAA,CAAAsB,GAAA;QACAC,MAAA;UACAnB,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,QAAA,OAAAA,QAAA;UACAE,OAAA,OAAAA,OAAA;UACAD,QAAA,OAAAA;QACA;MACA,GACAiB,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QAEA,KAAAvB,SAAA,GAAAuB,GAAA,CAAAG,OAAA;QACA,KAAAzB,KAAA,GAAAsB,GAAA,CAAAtB,KAAA;MAEA;IAEA;IACA0B,UAAAC,GAAA;MACA,KAAAd,IAAA,GAAAc,GAAA;MACA,KAAAf,YAAA;IACA;IACAgB,UAAA;MACA,KAAAhB,YAAA,SACA,KAAAC,IAAA;IACA;IACAgB,IAAAC,EAAA;MACAjC,OAAA,CAAAkC,MAAA,YAAAD,EAAA,EAAAT,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA;UACA,KAAAU,QA