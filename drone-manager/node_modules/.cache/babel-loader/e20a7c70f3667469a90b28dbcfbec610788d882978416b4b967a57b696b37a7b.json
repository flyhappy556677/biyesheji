{"ast":null,"code":"export default {\n  name: 'addMaintenance',\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      form: {},\n      rules: {} // 添加 rules 属性\n    };\n  },\n  created() {\n    const orderId = this.$route.query.id;\n    request.get(\"/order/getById/\" + orderId).then(res => {\n      this.form = res.data;\n      console.log(this.form);\n    });\n  },\n  methods: {\n    save() {\n      this.form.status = true;\n      request.post('/maintenance/save', {\n        ...this.form,\n        orderId: this.$route.query.id\n      }).then(res => {\n        console.log(this.form);\n        if (res.code === '200') {\n          this.$notify.success('更新成功');\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n      console.log(this.status);\n      this.form.status = true;\n      request.put('/order/statusupdate', this.form).then(res => {\n        if (res.code === '200') {\n          console.log(this.status);\n          this.$notify.success('更新成功');\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","localStorage","getItem","JSON","parse","form","rules","created","orderId","$route","query","id","request","get","then","res","console","log","methods","save","status","post","code","$notify","success","error","msg","put"],"sources":["src/views/addMaintenance.vue"],"sourcesContent":["<template>\r\n  <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" :inline=\"true\" style=\"width: 70%\" label-width=\"100px\">\r\n\r\n    <el-form-item label=\"操作用户\" prop=\"userName\">\r\n      <el-input v-model=\"form.userName\" placeholder=\"请输入无人机名称\"></el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"无人机名字\" prop=\"droneName\">\r\n      <el-input v-model=\"form.droneName\" placeholder=\"请输入无人机名字\"></el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"操作地址\" prop=\"address\">\r\n      <el-input style=\"width:400px\" v-model=\"form.address\"  placeholder=\"请输入描述\"></el-input>\r\n    </el-form-item>\r\n\r\n    <el-form-item label=\"手机号码\" prop=\"phone\">\r\n      <el-input style=\"width:400px\" v-model=\"form.phone\"  placeholder=\"请输入描述\"></el-input>\r\n    </el-form-item>\r\n\r\n\r\n    <el-form-item label=\"保养提交时间\" prop=\"createtime\">\r\n      <el-date-picker\r\n          v-model=\"form.createtime\"\r\n          type=\"date\"\r\n          value-format=\"yyyy-MM-dd\"\r\n          placeholder=\"保养提交时间\">\r\n      </el-date-picker>\r\n    </el-form-item>\r\n\r\n    <div style=\"text-align:center; margin-top:30px;\">\r\n      <el-button type=\"primary\" @click=\"save\">提交</el-button>\r\n      <el-button type=\"danger\">取消</el-button>\r\n    </div>\r\n\r\n  </el-form>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n  name:'addMaintenance',\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      form:{},\r\n      rules: {}  // 添加 rules 属性\r\n    }\r\n  },\r\n\r\n  created(){\r\n    const orderId = this.$route.query.id;\r\n    request.get(\"/order/getById/\"+orderId).then(res =>{\r\n      this.form =res.data\r\n      console.log(this.form)\r\n    })\r\n  },\r\n\r\n  methods:{\r\n\r\n    save(){\r\n      this.form.status=true\r\n\r\n      request.post('/maintenance/save',{...this.form,orderId:this.$route.query.id\r\n      }).then(res =>{\r\n        console.log(this.form)\r\n        if(res.code ==='200'){\r\n          this.$notify.success('更新成功')\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n\r\n        }\r\n      })\r\n      console.log(this.status)\r\n      this.form.status=true\r\n      request.put('/order/statusupdate',this.form).then(res =>{\r\n        if(res.code ==='200'){\r\n          console.log(this.status)\r\n          this.$notify.success('更新成功')\r\n\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n\r\n        }\r\n      })\r\n\r\n\r\n\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n\r\n"],"mappings":"AAuCA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAG,IAAA;MACAC,KAAA;IACA;EACA;EAEAC,QAAA;IACA,MAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,EAAA;IACAC,OAAA,CAAAC,GAAA,qBAAAL,OAAA,EAAAM,IAAA,CAAAC,GAAA;MACA,KAAAV,IAAA,GAAAU,GAAA,CAAAhB,IAAA;MACAiB,OAAA,CAAAC,GAAA,MAAAZ,IAAA;IACA;EACA;EAEAa,OAAA;IAEAC,KAAA;MACA,KAAAd,IAAA,CAAAe,MAAA;MAEAR,OAAA,CAAAS,IAAA;QAAA,QAAAhB,IAAA;QAAAG,OAAA,OAAAC,MAAA,CAAAC,KAAA,CAAAC;MACA,GAAAG,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,MAAAZ,IAAA;QACA,IAAAU,GAAA,CAAAO,IAAA;UACA,KAAAC,OAAA,CAAAC,OAAA;QACA;UACA,KAAAD,OAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;QAEA;MACA;MACAV,OAAA,CAAAC,GAAA,MAAAG,MAAA;MACA,KAAAf,IAAA,CAAAe,MAAA;MACAR,OAAA,CAAAe,GAAA,6BAAAtB,IAAA,EAAAS,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAO,IAAA;UACAN,OAAA,CAAAC,GAAA,MAAAG,MAAA;UACA,KAAAG,OAAA,CAAAC,OAAA;QAEA;UACA,KAAAD,OAAA,CAAAE,KAAA,CAAAV,GAAA,CAAAW,GAAA;QAEA;MACA;IAIA;EACA;AAEA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}