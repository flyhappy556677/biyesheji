{"ast":null,"code":"export default {\n  name: 'Maintenancelist',\n  data() {\n    return {\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\n      name: 'drone',\n      tableData: [],\n      total: 0,\n      cover: '',\n      pageNum: 1,\n      pageSize: 8\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/maintenance/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name,\n          droneNo: this.droneNo\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    resert() {\n      this.params = {\n        pageNum: 1,\n        pageSize: 10,\n        name: '',\n        droneNo: ''\n      };\n      this.load();\n    },\n    handelCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.params.pageNum = pageNum;\n      this.load();\n    },\n    del(id) {\n      request.delete(\"/operate/delete/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$notify.success('删除成功');\n          this.load();\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","localStorage","getItem","JSON","parse","tableData","total","cover","pageNum","pageSize","created","load","methods","request","get","params","droneNo","then","res","console","log","records","resert","handelCurrentChange","del","id","delete","code","$notify","success","error","msg"],"sources":["src/views/Maintenancelist.vue"],"sourcesContent":["\r\n<template>\r\n  <div>\r\n    <!-- 搜索表单 -->\r\n    <div>\r\n      <el-input style=\"width: 240px;\" placeholder=\"请输入无人机名称\" v-model=\"params.name\"></el-input>\r\n      <el-input style=\"width: 240px; margin-left:5px\" placeholder=\"请输入无人机编号\" v-model=\"params.droneNo\"></el-input>\r\n      <el-button @click=\"load\">搜索</el-button>\r\n      <el-button @click=\"resert\">重置</el-button>\r\n    </div>\r\n\r\n\r\n    <el-table :data=\"tableData\" style=\"width: 100%\" stripe row-key=\"id\" default-expand-all>\r\n      <el-table-column prop=\"id\" label=\"编号\"></el-table-column>\r\n      <el-table-column prop=\"userName\" label=\"申请维修人\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"申请地址\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"所填手机号\"></el-table-column>\r\n      <el-table-column prop=\"createtime\" label=\"申请时间\" ></el-table-column>\r\n\r\n      <el-table-column prop=\"note\" label=\"目前状态\">\r\n        <template v-slot=\"scope\">\r\n          <el-tag type=\"success\" v-if=\"scope.row.note === '已经保修完成'\">{{ scope.row.note }}</el-tag>\r\n          <el-tag type=\"warning\" v-if=\"scope.row.note === '需要进行保修'\">{{ scope.row.note }}</el-tag>\r\n        </template>\r\n      </el-table-column>\r\n\r\n\r\n      <el-table-column label=\"操作\" width=\"320\">\r\n        <template v-slot=\"scope\">\r\n          <div v-if=\"scope.row.status\">\r\n            <el-button type=\"primary\" size=\"7\" @click=\"$router.push('/MATman?id='+scope.row.id)\">进行维修</el-button>\r\n          </div>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <!-- 分页 -->\r\n    <div>\r\n      <el-pagination background layout=\"prev,pager,next\"\r\n                     :total=\"total\"\r\n                     :current-page=\"params.pageNum\"\r\n                     :page-size=\"params.pageSize\"\r\n                     @current-change=\"handelCurrentChange\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default{\r\n  name:'Maintenancelist',\r\n  data(){\r\n    return{\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      name:'drone',\r\n      tableData: [],\r\n      total:0,\r\n      cover:'',\r\n      pageNum:1,\r\n      pageSize:8,\r\n    }\r\n  },\r\n  created(){\r\n    this.load()\r\n\r\n  },\r\n  methods:{\r\n    load(){\r\n        this.request.get(\"/maintenance/page\",{\r\n          params:{\r\n            pageNum:this.pageNum,\r\n            pageSize:this.pageSize,\r\n            name:this.name,\r\n            droneNo:this.droneNo,\r\n          }\r\n        })\r\n            .then(res =>{\r\n              console.log(res)\r\n              this.tableData =res.data.records\r\n              this.total=res.data.total\r\n            })\r\n    },\r\n\r\n    resert(){\r\n      this.params={\r\n        pageNum:1,\r\n        pageSize:10,\r\n        name:'',\r\n        droneNo:''\r\n      }\r\n      this.load()\r\n    },\r\n\r\n    handelCurrentChange(pageNum){\r\n      console.log(pageNum)\r\n      this.params.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    del(id){\r\n      request.delete(\"/operate/delete/\"+id).then(res =>{\r\n        if(res.code ==='200'){\r\n          this.$notify.success('删除成功')\r\n          this.load()\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n\r\n\r\n\r\n  },\r\n}\r\n\r\n\r\n</script>"],"mappings":"AAmDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,YAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,YAAA,CAAAC,OAAA;MACAJ,IAAA;MACAO,SAAA;MACAC,KAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EAEA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAP,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAX,IAAA,OAAAA,IAAA;UACAkB,OAAA,OAAAA;QACA;MACA,GACAC,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAb,SAAA,GAAAa,GAAA,CAAAnB,IAAA,CAAAsB,OAAA;QACA,KAAAf,KAAA,GAAAY,GAAA,CAAAnB,IAAA,CAAAO,KAAA;MACA;IACA;IAEAgB,OAAA;MACA,KAAAP,MAAA;QACAP,OAAA;QACAC,QAAA;QACAX,IAAA;QACAkB,OAAA;MACA;MACA,KAAAL,IAAA;IACA;IAEAY,oBAAAf,OAAA;MACAW,OAAA,CAAAC,GAAA,CAAAZ,OAAA;MACA,KAAAO,MAAA,CAAAP,OAAA,GAAAA,OAAA;MACA,KAAAG,IAAA;IACA;IACAa,IAAAC,EAAA;MACAZ,OAAA,CAAAa,MAAA,sBAAAD,EAAA,EA