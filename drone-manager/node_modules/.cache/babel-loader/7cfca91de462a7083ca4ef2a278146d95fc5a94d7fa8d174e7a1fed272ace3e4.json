{"ast":null,"code":"export default {\n  name: \"Drone\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 8,\n      username: \"\",\n      nickname: \"\",\n      address: \"\",\n      form: {},\n      multipleSelection: [],\n      outerVisible: false,\n      roles: []\n    };\n  },\n  created() {\n    this.load();\n  },\n  methods: {\n    load() {\n      this.request.get(\"/drone/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n    hanleEdit(row) {\n      this.form = row;\n      this.outerVisible = true;\n    },\n    handleAdd() {\n      this.outerVisible = true, this.form = {};\n    },\n    del(id) {\n      this.request.delete(\"/drone/\" + id).then(res => {\n        if (res.data) {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    save() {\n      this.request.post(\"/drone\", this.form).then(res => {\n        if (res.data) {\n          this.$message.success(\"保存成功\");\n          this.outerVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n    reset() {\n      this.username = '', this.address = '', this.nickname = '', this.load();\n    },\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); //变为集合数组\n\n      this.request.post(\"/drone/del/batch\", ids).then(res => {\n        if (res.data) {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    exp() {\n      window.open(\"http://localhost:9090/user/export\");\n    },\n    handleImportSuccess() {\n      this.$message.success(\"导入成功\");\n      this.load();\n    }\n  }\n};","map":{"version":3,"names":["name","data","tableData","total","pageNum","pageSize","username","nickname","address","form","multipleSelection","outerVisible","roles","created","load","methods","request","get","params","then","res","console","log","records","hanleEdit","row","handleAdd","del","id","delete","$message","success","error","save","post","reset","handleSelectionChange","val","delBatch","ids","map","v","handleSizeChange","handleCurrentChange","exp","window","open","handleImportSuccess"],"sources":["src/views/Drone.vue"],"sourcesContent":["<template>\r\n  <div>\r\n\r\n    <div style=\"padding: 10px 0\">\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"username\"></el-input>\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\" class=\"ml-5\" v-model=\"nickname\"></el-input>\r\n      <el-input style=\"width: 200px\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\" class=\"ml-5\" v-model=\"address\"></el-input>\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\r\n      <el-button class=\"ml-5\" type=\"warning\" @click=\"reset\">重置</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button  type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我在想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定删除吗？\"\r\n          @confirm= \"delBatch\"\r\n      >\r\n        <el-button  type=\"danger\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\r\n      </el-popconfirm>\r\n\r\n      <el-upload action=\"http://localhost:9090/user/import\" :show-file-list=\"false\"  accept=\"xlsx\" :on-success=\"handleImportSuccess\" style=\"display: inline-block\">\r\n        <el-button  type=\"primary\" class=\"ml-5\">导入<i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n\r\n      <el-button  type=\"primary\" class=\"ml-5\" @click=\"exp\">导出<i class=\"el-icon-top\"></i></el-button>\r\n    </div>\r\n\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\" @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"140\"></el-table-column>\r\n      <el-table-column prop=\"role\" label=\"角色\"></el-table-column>\r\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\"></el-table-column>\r\n      <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"hanleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\r\n\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我在想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n          </el-popconfirm>\r\n\r\n        </template>\r\n      </el-table-column>\r\n\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[8, 10, 15, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n\r\n    </div>\r\n\r\n\r\n    <el-dialog title=\"无人机信息\" :visible.sync=\"outerVisible\" width=\"30%\">\r\n      <el-form label-width=\"80px\" size=\"smail\">\r\n        <el-form-item label=\"用户名\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"昵称\">\r\n          <el-input v-model=\"form.description\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"邮箱\">\r\n          <el-input v-model=\"form.company\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"电话\">\r\n          <el-input v-model=\"form.category\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"地址\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"outerVisible = false\">取 消</el-button>\r\n        <el-button @click=\"save\">提交</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n  </div>\r\n</template>\r\n<script >\r\nexport  default {\r\n  name:\"Drone\",\r\n  data(){\r\n    return{\r\n      tableData: [],\r\n      total:0,\r\n      pageNum:1,\r\n      pageSize:8,\r\n      username:\"\",\r\n      nickname:\"\",\r\n      address:\"\",\r\n      form:{},\r\n      multipleSelection:[],\r\n      outerVisible: false,\r\n      roles:[]\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods:{\r\n    load(){\r\n      this.request.get(\"/drone/page\",{\r\n        params:{\r\n          pageNum:this.pageNum,\r\n          pageSize:this.pageSize,\r\n          username:this.username,\r\n        }\r\n      })\r\n          .then(res =>{\r\n            console.log(res)\r\n            this.tableData =res.data.records\r\n            this.total=res.data.total\r\n          })\r\n\r\n    },\r\n    hanleEdit(row){\r\n      this.form=row\r\n      this.outerVisible=true\r\n    },\r\n    handleAdd(){\r\n      this.outerVisible =true,\r\n          this.form={}\r\n    },\r\n    del(id){\r\n      this.request.delete(\"/drone/\" +id).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    save(){\r\n      this.request.post(\"/drone\",this.form).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"保存成功\")\r\n          this.outerVisible=false\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    reset(){\r\n      this.username ='',\r\n          this.address='',\r\n          this.nickname='',\r\n          this.load()\r\n\r\n    },\r\n    handleSelectionChange(val){\r\n      console.log(val)\r\n      this.multipleSelection=val\r\n    },\r\n    delBatch(){\r\n      let ids = this.multipleSelection.map(v => v.id)//变为集合数组\r\n\r\n      this.request.post(\"/drone/del/batch\",ids).then(res =>{\r\n        if(res.data){\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        }else{\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSizeChange(pageSize){\r\n      this.pageSize=pageSize\r\n      this.load()\r\n\r\n    },\r\n    handleCurrentChange(pageNum){\r\n      this.pageNum=pageNum\r\n      this.load()\r\n    },\r\n    exp(){\r\n      window.open(\"http://localhost:9090/user/export\")\r\n    },\r\n    handleImportSuccess(){\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n.headerBg{\r\n  background-color: #eee!important;\r\n}\r\n</style>"],"mappings":"AA0GA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA;MACAC,QAAA;MACAC,OAAA;MACAC,IAAA;MACAC,iBAAA;MACAC,YAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAD,KAAA;MACA,KAAAE,OAAA,CAAAC,GAAA;QACAC,MAAA;UACAd,OAAA,OAAAA,OAAA;UACAC,QAAA,OAAAA,QAAA;UACAC,QAAA,OAAAA;QACA;MACA,GACAa,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAlB,SAAA,GAAAkB,GAAA,CAAAnB,IAAA,CAAAsB,OAAA;QACA,KAAApB,KAAA,GAAAiB,GAAA,CAAAnB,IAAA,CAAAE,KAAA;MACA;IAEA;IACAqB,UAAAC,GAAA;MACA,KAAAhB,IAAA,GAAAgB,GAAA;MACA,KAAAd,YAAA;IACA;IACAe,UAAA;MACA,KAAAf,YAAA,SACA,KAAAF,IAAA;IACA;IACAkB,IAAAC,EAAA;MACA,KAAAZ,OAAA,CAAAa,MAAA,aAAAD,EAAA,EAAAT,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAnB,IAAA;UACA,KAAA6B,QAAA,CAAAC,OAAA;UACA,KAAAjB,IAAA;QACA;UACA,KAAAgB,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAC,KAAA;MACA,KAAAjB,OAAA,CAAAkB,IAAA,gBAAAzB,IAAA,EAAAU,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAnB,IAAA;UACA,KAAA6B,QAAA,CAAAC,OAAA;UACA,KAAApB,YAAA;UACA,KAAAG,IAAA;QACA;UACA,KAAAgB,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAG,MAAA;MACA,KAAA7B,QAAA,OACA,KAAAE,OAAA,OACA,KAAAD,QAAA,OACA,KAAAO,IAAA;IAEA;IACAsB,sBAAAC,GAAA;MACAhB,OAAA,CAAAC,GAAA,CAAAe,GAAA;MACA,KAAA3B,iBAAA,GAAA2B,GAAA;IACA;IACAC,SAAA;MACA,IAAAC,GAAA,QAAA7B,iBAAA,CAAA8B,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAb,EAAA;;MAEA,KAAAZ,OAAA,CAAAkB,IAAA,qBAAAK,GAAA,EAAApB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAnB,IAAA;UACA,KAAA6B,QAAA,CAAAC,OAAA;UACA,KAAAjB,IAAA;QACA;UACA,KAAAgB,QAAA,CAAAE,KAAA;QACA;MACA;IACA;IACAU,iBAAArC,QAAA;MACA,KAAAA,QAAA,GAAAA,QAAA;MACA,KAAAS,IAAA;IAEA;IACA6B,oBAAAvC,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAU,IAAA;IACA;IACA8B