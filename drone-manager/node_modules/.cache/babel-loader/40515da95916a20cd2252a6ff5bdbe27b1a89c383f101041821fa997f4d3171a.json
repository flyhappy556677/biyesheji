{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport request from '@/utils/request';\nimport Cookie from 'js-cookie';\nexport default {\n  name: 'Editdrone',\n  data() {\n    return {\n      user: Cookie.get('user') ? JSON.parse(Cookie.get('user')) : {},\n      form: {\n        score: 10,\n        cover: ''\n      },\n      categories: [],\n      rules: {\n        name: [{\n          required: true,\n          message: '请输入无人机名称',\n          trigger: 'blur'\n        }],\n        droneNo: [{\n          required: true,\n          message: '请输入无人机标准码',\n          trigger: 'blur'\n        }],\n        score: [{\n          required: true,\n          message: '请输入积分',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n  created() {\n    request.get('/category/tree').then(res => {\n      this.categories = res.data;\n    });\n    const id = this.$route.query.id;\n    request.get(\"/drone/\" + id).then(res => {\n      this.form = res.data;\n      if (this.form.category) {\n        this.form.categories = this.form.category.split(\">\");\n      }\n    });\n  },\n  methods: {\n    handleCoverSuccess(res) {\n      if (res.code === '200') {\n        // this.$set(this.form,'cover',res.data)\n        this.form.cover = res.data;\n      }\n    },\n    save() {\n      request.put('/drone/update', this.form).then(res => {\n        console.log(this.form);\n        if (res.code === '200') {\n          this.$notify.success('更新成功');\n          this.$router.push(\"/dronelist\");\n        } else {\n          this.$notify.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["request","Cookie","name","data","user","get","JSON","parse","form","score","cover","categories","rules","required","message","trigger","droneNo","created","then","res","id","$route","query","category","split","methods","handleCoverSuccess","code","save","put","console","log","$notify","success","$router","push","error","msg"],"sources":["src/views/drone/DroneEdit.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>编辑无人机</h2>\r\n    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\" :inline=\"true\" style=\"width: 70%\" label-width=\"100px\">\r\n      <el-form-item label=\"无人机名称\" prop=\"name\">\r\n        <el-input v-model=\"form.name\" placeholder=\"请输入无人机名称\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"描述\" prop=\"description\">\r\n        <el-input style=\"width:400px\" v-model=\"form.description\" type=\"textarea\" placeholder=\"请输入描述\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"创造公司\" prop=\"company\">\r\n        <el-input v-model=\"form.company\" placeholder=\"请输入创造公司\"></el-input>\r\n      </el-form-item>\r\n\r\n\r\n      <el-form-item label=\"创造时间\" prop=\"createtime\">\r\n        <el-date-picker\r\n            v-model=\"form.createtime\"\r\n            type=\"date\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            placeholder=\"请输入创造时间\">\r\n        </el-date-picker>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"分类\" prop=\"category\">\r\n        <el-cascader\r\n            style=\"width:200px\"\r\n            :props=\"{value:'name' ,label:'name'}\"\r\n            v-model=\"form.categories\"\r\n            :options=\"categories\"\r\n        ></el-cascader>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"无人机编号\" prop=\"droneNo\">\r\n        <el-input v-model=\"form.droneNo\" placeholder=\"请输入无人机编号\"></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"所能极限飞行距离\" prop=\"score\">\r\n        <el-input v-model=\"form.score\" placeholder=\"请输入所能极限飞行距离\"></el-input>\r\n      </el-form-item>\r\n      <br>\r\n\r\n      <el-form-item label=\"图片\" prop=\"cover\">\r\n        <el-upload\r\n            class=\"avatar-uploader\"\r\n            :action=\"'http://localhost:9090/api/drone/file/upload?token='+this.user.token\"\r\n            :show-file-list=\"false\"\r\n            :on-success=\"handleCoverSuccess\"\r\n\r\n        >\r\n          <img v-if=\"form.cover\" :src=\"form.cover\" class=\"avatar\">\r\n          <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n        </el-upload>\r\n      </el-form-item>\r\n\r\n\r\n    </el-form>\r\n\r\n    <div style=\"text-align:center; margin: top 30px;\">\r\n      <el-button type=\"primary\" @click=\"save\">提交</el-button>\r\n      <el-button type=\"danger\">取消</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport request from '@/utils/request';\r\nimport Cookie from 'js-cookie';\r\nexport default {\r\n  name:'Editdrone',\r\n  data(){\r\n    return{\r\n      user:Cookie.get('user') ? JSON.parse(Cookie.get('user')) :{},\r\n      form:{score:10 , cover:''},\r\n      categories:[],\r\n      rules:{\r\n        name:[\r\n          {required:true, message:'请输入无人机名称',trigger:'blur'},\r\n        ],\r\n        droneNo:[\r\n          {required:true, message:'请输入无人机标准码',trigger:'blur'},\r\n        ],\r\n        score:[\r\n          {required:true, message:'请输入积分',trigger:'blur'},\r\n        ],\r\n      }\r\n    }\r\n  },\r\n  created(){\r\n\r\n    request.get('/category/tree').then(res =>{\r\n      this.categories =res.data\r\n    })\r\n\r\n    const id=this.$route.query.id\r\n\r\n    request.get(\"/drone/\"+id).then(res =>{\r\n      this.form =res.data\r\n      if(this.form.category){\r\n        this.form.categories = this.form.category.split(\">\")\r\n      }\r\n    })\r\n\r\n  },\r\n  methods:{\r\n    handleCoverSuccess(res){\r\n      if(res.code === '200'){\r\n        // this.$set(this.form,'cover',res.data)\r\n        this.form.cover=res.data\r\n      }\r\n    },\r\n    save(){\r\n      request.put('/drone/update',this.form).then(res =>{\r\n        console.log(this.form)\r\n        if(res.code ==='200'){\r\n          this.$notify.success('更新成功')\r\n          this.$router.push(\"/dronelist\")\r\n        }else{\r\n          this.$notify.error(res.msg)\r\n\r\n        }\r\n      })\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 178px;\r\n  height: 178px;\r\n  line-height: 178px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 178px;\r\n  height: 178px;\r\n  display: block;\r\n}\r\n</style>"],"mappings":";AAoEA,OAAAA,OAAA;AACA,OAAAC,MAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAH,MAAA,CAAAI,GAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAN,MAAA,CAAAI,GAAA;MACAG,IAAA;QAAAC,KAAA;QAAAC,KAAA;MAAA;MACAC,UAAA;MACAC,KAAA;QACAV,IAAA,GACA;UAAAW,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,OAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAN,KAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;IACA;EACA;EACAE,QAAA;IAEAjB,OAAA,CAAAK,GAAA,mBAAAa,IAAA,CAAAC,GAAA;MACA,KAAAR,UAAA,GAAAQ,GAAA,CAAAhB,IAAA;IACA;IAEA,MAAAiB,EAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAF,EAAA;IAEApB,OAAA,CAAAK,GAAA,aAAAe,EAAA,EAAAF,IAAA,CAAAC,GAAA;MACA,KAAAX,IAAA,GAAAW,GAAA,CAAAhB,IAAA;MACA,SAAAK,IAAA,CAAAe,QAAA;QACA,KAAAf,IAAA,CAAAG,UAAA,QAAAH,IAAA,CAAAe,QAAA,CAAAC,KAAA;MACA;IACA;EAEA;EACAC,OAAA;IACAC,mBAAAP,GAAA;MACA,IAAAA,GAAA,CAAAQ,IAAA;QACA;QACA,KAAAnB,IAAA,CAAAE,KAAA,GAAAS,GAAA,CAAAhB,IAAA;MACA;IACA;IACAyB,KAAA;MACA5B,OAAA,CAAA6B,GAAA,uBAAArB,IAAA,EAAAU,IAAA,CAAAC,GAAA;QACAW,OAAA,CAAAC,GAAA,MAAAvB,IAAA;QACA,IAAAW,GAAA,CAAAQ,IAAA;UACA,KAAAK,OAAA,CAAAC,OAAA;UACA,KAAAC,OAAA,CAAAC,IAAA;QACA;UACA,KAAAH,OAAA,CAAAI,KAAA,CAAAjB,GAAA,CAAAkB,GAAA;QAEA;MACA;IAEA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}